{
    "manifest_version":2,
    "name":"Simple Adblocker",
    "version":"1.0",
    "author":"alogico",
    "description":"this is a simple Ad blocker which doesn't impact the DOM",
    "icons":{
        "16": "icons/logo_16x16.png",
        "32": "icons/logo_32x32.png",
        "128": "icons/logo_128x128.png"

    },
    "background":{
        "service_worker":"background.js"
        
    },
    "permissions":
    [
        "webRequest", "webRequestBlocking", "<all_urls>"
    ],
    "browser_action":{
        "default_popup":"popup.html"
    }
    
    
}